<SignUp>
    name: "SignUp"
    fname: fname
    lname: lname
    email: email
    dob: dob
    npwd: npwd
    cpwd: cpwd
    
    MDBoxLayout:
        md_bg_color: "white"
        orientation: "vertical"
        size: root.width, root.height
        padding: 80
        spacing: 10
        
        MDTextField:
            id: fname
            hint_text: "*First Name"
            mode: "rectangle"
            #helper_text: "Required"
            helper_text_mode: "on_error"
            required: True
            pos_hint: {'center_x':.5,'center_y':0}
            size_hint_y: None
            size_hint_x: .6
            height: "30dp"
            focus: True
            multiline: False
            on_text_validate: root.submitRequest()
            
        MDTextField:
            id: lname
            hint_text: "*Last Name"
            mode: "rectangle"
            helper_text_mode: "on_error"
            required: True
            #helper_text: "It is mandatory to signup."
            helper_text_mode: "on_error"
            pos_hint: {'center_x':.5,'center_y':0}
            size_hint_y: None
            size_hint_x: .6
            height: "30dp"
            focus: True
            multiline: False
            on_text_validate: root.submitRequest()
        MDTextField:
            id: email
            hint_text: "*E-mail"
            icon_left: "email"
            mode: "rectangle"
            helper_text_mode: "on_error"
            required: True
            #helper_text: "It is mandatory to signup."
            helper_text_mode: "on_error"
            pos_hint: {'center_x':.5,'center_y':0}
            size_hint_y: None
            size_hint_x: .6
            height: "30dp"
            focus: True
            multiline: False
            on_text_validate: root.submitRequest()
        MDTextField:
            id: dob
            hint_text: "Date of Birth"
            icon_left: "calendar"
            mode: "rectangle"
            helper_text: "It is mandatory to signup."
            helper_text_mode: "on_error"
            pos_hint: {'center_x':.5,'center_y':0}
            size_hint_y: None
            size_hint_x: .6
            height: "30dp"
            focus: True
            multiline: False
            on_text_validate: root.submitRequest()
        MDTextField:
            id: npwd
            hint_text: "*New Password"
            icon_left: "key-outline"
            mode: "rectangle"
            helper_text_mode: "on_error"
            required: True
            #helper_text: "It is mandatory to signup."
            helper_text_mode: "on_error"
            pos_hint: {'center_x':.5,'center_y':0}
            size_hint_y: None
            size_hint_x: .6
            height: "30dp"
            focus: True
            multiline: False
            on_text_validate: root.submitRequest()
        MDTextField:
            id: cpwd
            hint_text: "*Confirm Password"
            icon_left: "key-outline"
            mode: "rectangle"
            helper_text_mode: "on_error"
            required: True
            #helper_text: "It is mandatory to signup."
            helper_text_mode: "on_error"
            pos_hint: {'center_x':.5,'center_y':0}
            size_hint_y: None
            size_hint_x: .6
            height: "30dp"
            focus: True
            multiline: False
            on_text_validate: root.submitRequest()
        
        
        MDFillRoundFlatButton:
            text: "Sign Up"
            size_hint: .4,0.3
            text_color: "white"
            font_style: "Button"
            line_color: 0, 0, 0, 0
            pos_hint: {"center_x": .5, "center_y": .1}
            
            on_press: 
                print("Signup Successful!")
                root.btn()
                
                root.manager.current = 'Login' if (root.t ==True) else root.btn()
                root.manager.transition.direction="left"
             
       

<Login>
    name: "Login"
    email: email
    pwd: pwd
    
    MDBoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        md_bg_color: "white"
        padding: 180
        spacing: 10
       
        MDTextField:
            id: email
            hint_text: "Enter e-mail address..."
            icon_left: "email"
            mode: "rectangle"
            #helper_text: "It is mandatory to login."
            helper_text_mode: "on_error"
            required: True
            helper_text_mode: "on_error"
            pos_hint: {'center_x':.5,'center_y':0}
            size_hint_y: None
            size_hint_x: .7
            height: "30dp"
            focus: True
            multiline: False
            on_text_validate: root.submitRequest()
        	
        MDTextField:
            id: pwd
            hint_text: "Password"
            icon_left: 'key-outline'
            icon_right: "eye-off"
            mode: "rectangle"
            helper_text_mode: "on_error"
            required: True
            #helper_text: "It is mandatory to login."
            helper_text_mode: "on_error"
            pos_hint: {'center_x':.5,'center_y':0}
            size_hint_y: None
            size_hint_x: .7
            height: "30dp"
            focus: True
            multiline: False
            on_text_validate: root.submitRequest()
       
        MDFillRoundFlatIconButton:
            text: "Login"
            icon: "login"
            icon_size: "20sp"
            size_hint: (.4, None)
            text_color: "white"
            font_style: "Button"
            line_color: 0, 0, 0, 0
            theme_icon_color: "Custom"
            icon_color: "white"
            pos_hint: {"center_x": .5, "center_y": .5}
            on_release: 
                print("Login Successful!")
                root.validate()
                root.manager.current = 'Home' if (root.t ==True) else root.validate()
                root.manager.transition.direction="down"
       
        Label:
        	text: "or"
        	color: (0,0,0,1)
        	font_name: "Arial"
        	pos_hint: {'center_x':.5,'center_y':0}
        	font_size:20
        
        MDFillRoundFlatButton:
            text: "Sign Up"
            size_hint: (.4, None)
            text_color: "white"
            font_style: "Button"
            line_color: 0, 0, 0, 0
            pos_hint: {"center_x": .5, "center_y": .1}
            on_release: 
                print("Signup Successful!")
                root.manager.current = 'SignUp'
                #app.root.current="SignUp"
                root.manager.transition.direction="right"
    	
